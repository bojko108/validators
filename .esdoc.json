{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "accessor": {
          "access": ["public", "protected", "private"]
        }
      }
    },
    {
      "name": "esdoc-lint-plugin",
      "option": {
        "enable": false
      }
    }
  ]
}
